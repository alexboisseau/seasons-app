{
  "name": "m1-react",
  "version": "1.0.0",
  "description": "Seasons App",
  "source": "src/index.html",
  "scripts": {
    "start": "parcel",
    "lint": "eslint src",
    "test": "jest --watchAll --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexboisseau/m1-react.git"
  },
  "author": "Alex Boisseau",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alexboisseau/m1-react/issues"
  },
  "homepage": "https://github.com/alexboisseau/m1-react#readme",
  "dependencies": {
    "date-fns": "^2.28.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.16.7",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.9.0",
    "eslint-config-react-app": "^7.0.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.3",
    "parcel": "^2.3.1",
    "postcss": "^8.4.6",
    "tailwindcss": "^3.0.20"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    },
    "coverageThreshold": {
      "global": {
        "statements": 66,
        "branches": 70,
        "functions": 75,
        "lines": 68
      }
    }
  }
}
